[Type] == Amulet && [Quality] == rare # [SorceressSkills] == 2 && [FCR] == 10 && [MaxHp]+[MaxMana] >= 100



// ##############################################################################################################################################
// ##### 3- Rare Helms ########################################################################################################################
// ##############################################################################################################################################
